#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'g12-prii3-turtlesim','console_scripts','mover_tortuga'
__requires__ = 'g12-prii3-turtlesim'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('g12-prii3-turtlesim', 'console_scripts', 'mover_tortuga')()
    )
